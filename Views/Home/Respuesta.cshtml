@if(ViewBag.gano){
    <a href='@Url.Action("Jugar")'>
    <div class="Respuesta__correcta">
        <h1>Correcto!</h1>
        <p>Click para continuar</p>  
    </div>
    </a> 
}else{
    <a href='@Url.Action("Fin")'>
    <div class="Respuesta__incorrecta">
    <h1>Incorrecto! :/</h1>
    <p>Click para volver al inicio</p>  
    </div>
    </a>
}


